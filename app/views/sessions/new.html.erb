<% provide(:title, "登录") %>
<h1>登录</h1>
<div class="row">
  <div class="span6 offset3">
	<%= form_for(:session, url: sessions_path) do |f| %>
	<%= f.label :用户名 %>
	<%= f.text_field :name %>
	<%= f.label :密码 %>
	<%= f.password_field :password %>
	<%= f.submit "登录", class: "btn btn-large btn-primary" %>
	<% end %>
   </div>
</div>
